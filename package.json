{
	"name": "bbchallenge",
	"version": "0.0.1",
	"scripts": {
		"proxy": "cors-backdoor --target \"${API_BASE_URL:-https://api.bbchallenge.org}\" --port 4000 --config cors-backdoor.config.cjs",
		"dev:base": "VITE_API_URL=http://localhost:4000/ svelte-kit dev",
		"dev": "run-p proxy dev:base",
		"dev:local": "VITE_API_URL=http://127.0.0.1:55055 svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.13.0",
		"@typescript-eslint/parser": "^5.13.0",
		"autoprefixer": "^10.4.4",
		"eslint": "^8.10.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"postcss-cli": "^9.1.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"svelte": "^3.46.4",
		"svelte-check": "^2.4.5",
		"svelte-preprocess": "^4.10.4",
		"tailwindcss": "^3.0.23",
		"tslib": "^2.3.1",
		"typescript": "~4.5.5",
		"vite": "^2.8.6"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.29",
		"@tailwindcss/typography": "^0.5.2",
		"axios": "^0.26.0",
		"cors-backdoor": "^1.0.6",
		"mdsvex": "^0.10.5",
		"netlify-cli": "^9.13.0",
		"rehype-katex": "^6.0.2",
		"rehype-mathjax": "^4.0.2",
		"remark-footnotes": "^2.0.0",
		"remark-math": "^5.1.1",
		"remark-toc": "^8.0.1",
		"run-p": "^0.0.0",
		"svelte-seo": "^1.4.0",
		"yarn-run-all": "^3.1.1"
	}
}
